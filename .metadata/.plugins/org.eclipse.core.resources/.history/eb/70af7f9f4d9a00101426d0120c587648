package com.aula01;

public class variaveis {
	//Indicação da classe inicial
	public static void main(String[] args) {
		//Criação de variavel
		String nome = "Danilo";
		int idade = 23;
		double nota = 10.2;
		float altura = 1.88f;
		boolean resultado = true;
		final double PI = 3.19;
		char sexo = 'm';
		
		idade = 45;
		
		
				
		System.out.println("Olá mundo!");
		System.out.println(nome+" sua idade é "+idade);
		
				
	}
}
